/* 幻灯片 滑块 */
.swiper-img {
	width: 750rpx;
	height: 300rpx;
}
.swiper-flex {
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	        justify-content: center;
}
.swiper-item {
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	        justify-content: center;
}
/* 搜索框 start */
.tip-text {
	text-align: center;
	font-size: 32rpx;
	color: #4d4d4d;
	margin-top: 10rpx;
}
.input-view {
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	direction: row;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	        justify-content: center;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	margin: 10rpx 20rpx 20rpx 20rpx;
	/* height: 80upx; */
	border: 1px solid #72c69c;
	border-radius: 50rpx;
	box-shadow: 10rpx 10rpx 20rpx #c5c5c5;
	/* box-shadow: 10upx 10upx 12upx #888888; */
	/* box-shadow: 0 0 30upx #888888; */
}
.input-view-camera,
.input-view-speech,
.input-view-search {
	/* border: 1px solid red; */
	/* border-radius: 20upx; */
}
.input-view-camera {}
.input-view-item {
	height: 80rpx;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	padding: 10rpx;
}
.input-search {
	height: 80rpx;
	width: 320rpx;
	text-align:center;
}
.search-img {
	width: 80rpx;
	height: 80rpx;
}
.font-search {
	/* border: 2px solid red; */
	width: 80rpx;
	padding: 10rpx;
	color: #72c69c;
	font-weight: bold;
}
/* 搜索框 end */
/*  主要分类 面板  start*/
.main-classify {
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	direction: center;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	        justify-content: center;
}
.main-img {
	width: 200rpx;
	height: 200rpx;
	padding: 20rpx;
}
/* 主要分类 面板 end */
/* 录音 start */
.recording-box {
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-orient: horizontal;
	-webkit-box-direction: normal;
	-webkit-flex-direction: row;
	        flex-direction: row;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	        justify-content: center;
}
#canvas {
	/* border: 2upx solid red; */
	position: relative;
	width: 300rpx;
	height: 300rpx;
	z-index: 10;
}
.recording-button {
	position: absolute;
	box-sizing: border-box;
	top: 50rpx;
	left: 50rpx;
	display: inline-block;
	width: 200rpx;
	height: 200rpx;
	border: 1px dashed #1296DB;
	border-radius: 200rpx;
	background: url(data:image/png;base64,) no-repeat 50% 50%;
	background-size: 80% 80%;
	z-index: 100;
}
/* 录音 end */
.simpleTips{
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-orient: horizontal;
	-webkit-box-direction: normal;
	-webkit-flex-direction: row;
	        flex-direction: row;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	        justify-content: center;
	font-size: 28rpx;
	/* color: #72c69c; */
	color: #666666;
	/* text-decoration: underline; */
}

