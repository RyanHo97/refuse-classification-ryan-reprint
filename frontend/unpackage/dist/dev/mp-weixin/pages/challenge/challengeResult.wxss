.title {
	text-align: center;
	margin: 20rpx auto;
	font-size: 40rpx;
}
.level {
	text-align: center;
	margin-bottom: 20rpx;
	font-size: 40rpx;
}
.summary {
	text-align: center;
	margin-bottom: 20rpx;
	margin: 10rpx 5%;
}

/* 表格列表 start */
.table {
	padding: 0 5%;
	font-size: 30rpx;
}
.table-item {
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-orient: horizontal;
	-webkit-box-direction: normal;
	-webkit-flex-direction: row;
	        flex-direction: row;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	        justify-content: center;
	padding: 6rpx;
	height: 50rpx;
}
.icon-img {
	width: 50rpx;
	height: 50rpx;
	/* padding: 4upx; */
}
.table-item-icon {
	width: 10%;
	text-align: center;
}
.garbage-name {
	width: 50%;
	line-height: 50rpx;
	margin-left: 20rpx;
}
.through {
	text-decoration: line-through;
}
.selected {
	width: 25%;
	line-height: 50rpx;
	text-align: center;
}
.result {
	width: 25%;
	text-align: center;
	line-height: 50rpx;
}
.gray {
	background-color: #f0f0f0;
}
.garbage-gan-item {
	color: rgb(44, 43, 39);
}
.garbage-shi-item {
	color: rgb(102, 63, 52);
}
.garbage-huishou-item {
	color: rgb(36, 71, 131);
}
.garbage-youhai-item {
	color: rgb(229, 49, 34);
}

/* 表格列表 end */
.button-show {
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-orient: horizontal;
	-webkit-box-direction: normal;
	-webkit-flex-direction: row;
	        flex-direction: row;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	        justify-content: center;
	margin-top: 20rpx;
}
.goonButton {
	padding: 0 60rpx;
}
.button-left {
	margin-right: 60rpx;
}
.level {
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-orient: horizontal;
	-webkit-box-direction: normal;
	-webkit-flex-direction: row;
	        flex-direction: row;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	        justify-content: center;
}
.text-left{
	font-weight: bold;
	font-size: 44rpx;
}
.text-right {
	margin-left: 20rpx;
	color: #FF6600;
	border-bottom: 4rpx solid #FF6600;
}
.level1{
	color: red;
}
.level2{
	color: #737373;
}
.level3{
	color: #e96900;
}
.level4{
	color: #1296DB;
}
.level5{
	color: #72C69C;
}

